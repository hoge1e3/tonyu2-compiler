native Promise;
native setTimeout;
native Tonyu;

\waitFor(f) {
    if (_thread) {
        return __await f;
    }
    return f;
}
\update(t) {
    waitFor(new Promise(\(s) {
        setTimeout(s,t||50);
    }));
}
\appear(o) {
    var t=Tonyu.thread();
    t.apply(o,"main",[]);
    t.steps();
    return o;
}
